package MarvelousRing

import ObjectIdGenerator
import ItemDefinition

public constant MARVELOUS_RING_ID = compiletime(ITEM_ID_GEN.next())
public constant MARVELOUS_RING_NAME = "Marvelous Ring"

@compiletime function gen()
	new BaseItemDefinition(MARVELOUS_RING_ID, 'rst1', ItemType.COMMON)
		..setGoldCost(150)
		..setLumberCost(0)
		..setAllStatsBonus(3)
