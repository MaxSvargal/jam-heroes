package LuckyBracers

import ObjectIdGenerator
import ItemDefinition

public constant LUCKY_BRACERS_ID = compiletime(ITEM_ID_GEN.next())
public constant LUCKY_BRACERS_NAME = "Lucky Bracers"

@compiletime function gen()
	new BaseItemDefinition(LUCKY_BRACERS_ID, 'rst1', ItemType.COMMON)
		..setName(LUCKY_BRACERS_NAME)
		..setManaRegenerationBonus(3)
		..setGoldCost(100)
		..setLumberCost(0)
		..setDescription("desc")
	
	
		..setIcon("BTNHasteTier1")
