package Staff

import ObjectIdGenerator
import ItemDefinition

public constant STAFF_ID = compiletime(ITEM_ID_GEN.next())
public constant STAFF_NAME = "Staff"

@compiletime function gen()
	new BaseItemDefinition(STAFF_ID, 'rst1', ItemType.COMMON)
		..setGoldCost(100)
		..setLumberCost(0)
		..setAttackBonus(15)
		..setName(STAFF_NAME)
		..setDescription("desc")
		..setTooltipBasic("tooltip basic")
		..setTooltipExtended("tooltip extended")
		..setIcon("BTNHasteTier1")
