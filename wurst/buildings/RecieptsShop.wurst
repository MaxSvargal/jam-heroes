package RecieptsShop

import UnitObjEditing
import UnitIds
import ObjectIdGenerator

import UncommonItems

public constant RECIEPTS_SHOP_ID = compiletime(UNIT_ID_GEN.next())

@compiletime function gen()
	new BuildingDefinition(RECIEPTS_SHOP_ID, UnitIds.goblinmerchant)
		..setName("Uncommon Reciepts Shop")
		..setNeutralBuildingShowsMinimapIcon(false)
		..setItemsSold(uncommonRecieptsDropTable.asString())
