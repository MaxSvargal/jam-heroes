package BattleForCamp

import Battle
import BattleArea
import CreepCamp
import CreepCampGroup
import PlayerConfigurator

public class BattleForCamp extends Battle
	CreepCamp creepCamp

	construct(CreepCampGroup campGroup, BattleArea battleArea, CreepCamp creepCamp)
		super(campGroup, battleArea)
		this.creepCamp = creepCamp

	override function onVictory(PlayerConfigurator p)
		super.onVictory(p)
		creepCamp.toRestore()
